{
  "build": {
    "tasks_filename": "Game/spatialos.unreal.worker.build.json"
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "UnrealWorker"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 2
      }
    },
    "streaming_query": [],
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkout_all_initially": true
    }
  },
  "managed": {
    "windows": {
      "artifact_name": "UnrealWorker@Windows.zip",
      "command": "RpgDemo/Binaries/Win64/RpgDemoServer.exe",
      "arguments": [
        "+appName",
        "${IMPROBABLE_PROJECT_NAME}",
        "+receptionistIp",
        "${IMPROBABLE_RECEPTIONIST_IP}",
        "+receptionistPort",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "+workerType",
        "${IMPROBABLE_WORKER_NAME}",
        "+workerId",
        "${IMPROBABLE_WORKER_ID}",
        "+linkProtocol",
        "RakNet",
        "log=${IMPROBABLE_LOG_FILE}"
      ]
    },
    "linux": {
      "artifact_name": "UnrealWorker@Linux.zip",
      "command": "RpgDemoServer.sh",
      "arguments": [
        "+appName",
        "${IMPROBABLE_PROJECT_NAME}",
        "+receptionistIp",
        "${IMPROBABLE_RECEPTIONIST_IP}",
        "+receptionistPort",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "+workerType",
        "${IMPROBABLE_WORKER_NAME}",
        "+workerId",
        "${IMPROBABLE_WORKER_ID}",
        "+linkProtocol",
        "RakNet",
        "log=${IMPROBABLE_LOG_FILE}"
      ]
    }
  },
  "external": {
    "default": {
      "run_type": "EXECUTABLE_ZIP",
      "windows": {
        "artifact_name": "UnrealWorker@Windows.zip",
        "command": "RpgDemo/Binaries/Win64/RpgDemoServer.exe",
        "arguments": [
          "+workerType",
          "UnrealWorker",
          "log=unrealWorker.log",
        ]
      }
    }
  }
}
